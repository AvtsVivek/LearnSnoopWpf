<local:SnoopMainBaseWindow x:Class="FindViewModel.MalDll.Windows.InjectedWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FindViewModel.MalDll.Windows"
        mc:Ignorable="d"
        Title="InjectedWindow" Height="450" Width="800">
    <StackPanel Orientation="Vertical">
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
            <TextBlock HorizontalAlignment="Center" x:Name="secondTextBox" Margin="10" FontSize="25" >Process Id:</TextBlock>
            <TextBlock HorizontalAlignment="Center" x:Name="resultTextBox" Margin="10" FontSize="25" >0</TextBlock>
        </StackPanel>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center" Orientation="Horizontal">
            <TextBlock HorizontalAlignment="Center" x:Name="secondProcessNameTextBox" Margin="10" FontSize="25" >Process Name:</TextBlock>
            <TextBlock HorizontalAlignment="Center" x:Name="resultProcessNameTextBox" Margin="10" FontSize="25" ></TextBlock>
        </StackPanel>
    </StackPanel>
</local:SnoopMainBaseWindow>
